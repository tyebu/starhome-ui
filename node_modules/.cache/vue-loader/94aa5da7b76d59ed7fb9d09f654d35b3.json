 key=\"1\">\n            <router-link :to=\"{ name: 'settings' }\">\n              <a-icon type=\"setting\"/>\n              <span>账户设置</span>\n            </router-link>\n          </a-menu-item>\n          <a-menu-item key=\"2\" disabled>\n            <a-icon type=\"setting\"/>\n            <span>测试</span>\n          </a-menu-item>\n          <a-menu-divider/>\n          <a-menu-item key=\"3\">\n            <a href=\"javascript:;\" @click=\"handleLogout\">\n              <a-icon type=\"logout\"/>\n              <span>退出登录</span>\n            </a>\n          </a-menu-item>\n        </a-menu>\n      </a-dropdown>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NoticeIcon from '@/components/NoticeIcon'\nimport { mapActions, mapGetters } from 'vuex'\n\nexport default {\n  name: 'UserMenu',\n  components: {\n    NoticeIcon\n  },\n  computed: {\n    ...mapGetters(['nickname', 'avatar'])\n\n  },\n  methods: {\n    ...mapActions(['Logout']),\n    handleLogout () {\n      this.$confirm({\n        title: '提示',\n        content: '真的要注销登录吗 ?',\n        onOk: () => {\n          return this.Logout({}).then(() => {\n            setTimeout(() => {\n              window.location.reload()\n            }, 16)\n          }).catch(err => {\n            this.$message.error({\n              title: '错误',\n              description: err.message\n            })\n          })\n        },\n        onCancel () {\n        }\n      })\n    }\n  }\n}\n</script>\n"]}]}